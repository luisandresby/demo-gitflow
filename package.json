{
  "name": "demo-git-flow",
  "private": true,
  "version": "0.0.1-0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "element-plus": "^2.9.0",
    "sass": "^1.82.0",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^9.0.3",
    "@vitejs/plugin-vue": "^5.2.1",
    "release-it": "^17.10.0",
    "unplugin-auto-import": "^0.18.6",
    "unplugin-vue-components": "^0.27.5",
    "vite": "^6.0.1"
  },
  "release-it": {
    "git": {
      "commitMessage": "release ${version}"
    },
    "npm": {
      "publish": false
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "infile": "CHANGELOG.md",
        "ignoreRecommendedBump": true,
        "preset": {
          "name": "conventionalcommits",
          "types": [
            {
              "type": "feat",
              "section": "Features"
            },
            {
              "type": "fix",
              "section": "Bug Fixes"
            },
            {
              "type": "internal",
              "section": "Internal"
            },
            {
              "type": "chore",
              "section": "Internal"
            }
          ]
        }
      }
    }
  }
}
